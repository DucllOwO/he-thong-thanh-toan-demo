{
  "expo": {
    "name": "BxProject",
    "slug": "BxProject",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSFaceIDUsageDescription": "Allow $(PRODUCT_NAME) to use Face ID."
      }
    },
    "plugins": [
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow $(PRODUCT_NAME) to use Face ID."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "9ce375f2-75c8-42f9-8e00-9084d9fc1ba2"
      }
    },
    "owner": "nguyenduc147862",
    "android": {
      "package": "com.nguyenduc147862.BxProject"
    }
  }
}
